<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.baizhi.dao.AdminDao">
    <select id="queryByUsername" parameterType="String" resultType="cn.baizhi.entity.Admin">
        select id,username,password,status from yxadmin where username=#{username}
    </select>

</mapper>